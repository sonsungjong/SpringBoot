package com.example.myweb.model;

import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name="user")
public class User {
	
	private int no;
	
	private String id;
	
	private String pw;
	
	public User() {}

	public User(String id, String pw) {
		super();
		this.id = id;
		this.pw = pw;
	}
	
	
	
	
}
